#!/bin/bash

echo "*******************************"
echo "* Step 3: Generate CDF"
echo "*******************************"
echo 

BINDIR="$(dirname $0)"
ETCDIR="${BINDIR}/../etc"
LIBDIR="${BINDIR}/../lib"
. $BINDIR/sofcdfgen $*
CDF="$TOUT"
echo "** CDF=$CDF"

echo "*******************************"
echo "* Step 4: Program JIC to EPCS4"
echo "*******************************"
echo 

set -x
quartus_pgm "$CDF"
set +x

